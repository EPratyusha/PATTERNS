#include <stdio.h>

int main()
{
    int rows=10,i,j;
    for(i=1;i<rows;i++)
    {
        for(j=1;j<=rows;j++)
        {
            if(j==1)
            printf("*");
            else if(i<=(rows/2) && j==(rows-i))
            printf("*");
            else if(i>=(rows/2) && i==j)
            printf("*");
            else
            printf(" ");
        }
        printf("\n");
    }
    return 0;
}
