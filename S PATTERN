#include <stdio.h>

int main()
{
    int rows=10,i,j;
    for(i=1;i<=rows;i++)
    {
        for(j=1;j<=rows;j++)
        {
            if(i==1 || i==(rows/2) || i==rows)
            printf("*");
            else if(i<=(rows/2) && j==1)
            printf("*");
            else if(i>(rows/2) && j==rows)
            printf("*");
            else
            printf(" ");
        }
        printf("\n");
    }
    return 0;
}
