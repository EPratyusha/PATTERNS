#include <stdio.h>
int main() 
{
    int rows=10,i,j;
    for(i=1;i<=rows;i++)
    {
        for(j=1;j<=rows-i;j++)
        {
            if(j==i)
              printf("*");
            else if(j == rows-i)
              printf("*");
            else
              printf(" ");
              
        }
        printf("\n");
    }
    return 0;
        
}
